<AnchorPane minHeight="300.0" minWidth="500.0" style="-fx-border-color: FFFFFF; -fx-border-width: 6;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.view.DataConfigurationDialogController">
