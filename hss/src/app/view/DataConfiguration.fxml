                  </Button>
                  <Button defaultButton="true" mnemonicParsing="false" onAction="#handleOk" text="OK">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
              </buttons>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
